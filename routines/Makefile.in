SHELL = /bin/sh

AR = @AR@
ARFLAGS = @ARFLAGS@
CC = @CC@
CFLAGS = @CFLAGS@
RANLIB = @RANLIB@

ALL = get_series.o get_multi_series.o myfgets.o\
      search_datafile.o test_outfile.o check_alloc.o\
      scan_help.o check_option.o what_i_do.o\
